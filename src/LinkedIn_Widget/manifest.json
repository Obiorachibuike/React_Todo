{
    "manifest_version": 3,
    "name": "LinkedIn Profile Enhancer",
    "version": "1.0",
    "description": "Injects a widget into LinkedIn profiles showing match score and status",
    "permissions": ["storage"],
    "host_permissions": ["https://www.linkedin.com/*"],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/in/*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
